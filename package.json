    "jquery": "^3.7.1",
    "vue": "~2.6.14",
    "vue-chartjs": "^4.1.2",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.10",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^9.17.0",
    "html-loader": "^1.3.0",
